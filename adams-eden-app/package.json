    "eas:preview:ios": "npx eas-cli build -p ios --profile preview",
    "eas:prod:android": "npx eas-cli build -p android --profile production",
    "eas:prod:ios": "npx eas-cli build -p ios --profile production",
    "eas:submit:android": "npx eas-cli submit -p android --profile production",
    "eas:submit:ios": "npx eas-cli submit -p ios --profile production"
